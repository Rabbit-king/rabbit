class Solution {
	public void intput() {
		System.out.println("输入int型数组");
		
		ArrayList<Integer> A=new ArrayList<Integer>();
		
		Scanner S=new Scanner(System.in);
		
		while(S.hasNext("\n")) {
			A.add(Integer.valueOf(S.next()));
		}
		S.close();
	}
