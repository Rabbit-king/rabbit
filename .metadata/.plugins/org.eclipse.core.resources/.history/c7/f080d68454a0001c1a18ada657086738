package joesefu;



//约瑟夫问题，有n个人，从1开始报数，报到3的人退出，还剩下哪两个人

public class first {

	
}

class Node{
	int val;
	Node next;
	public Node(int val, Node next) {
		this.val = val;
		this.next = next;
	}
}

class Link{
	Node head=new Node(0, null);
	Node last=head;
	
	public void add(int i) {
		last.next=new Node(i, null);
		last=last.next;
	}
}