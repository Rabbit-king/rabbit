package first;

//计算数组中最大的子序列和

public class first {
	
}

class solution{
	public int sumMax(int a[]) {
		if(a.length>0) {
			int max=a[0];
			for(int i=0;i<a.length;i++) {
				max=max>sumMax(a,max,i+1)?max:sumMax(a,max,i+1);
			}
			return max;
		}
		else {
			return 0;
		}
	}
	
	public int sumMax(int a[],int max,int index) {
		if(index>=a.length) {
			return 0;
		}
		
		return 0;
	}
}