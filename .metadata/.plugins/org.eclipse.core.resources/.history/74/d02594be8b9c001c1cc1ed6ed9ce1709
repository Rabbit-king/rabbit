package first;

public class first {
public static void main(String[] args) {
	Solution S=new Solution();
	System.out.println(S.isHappy(19));
}
}

class Solution {
    public boolean isHappy(long n) {

		long sum=0;
		do {
		while(n>0) {
			sum=sum+(n%10)*(n%10);
			n=n/10;
		}}while(n>0);
		
		if(sum==1)
			return true;
		else
			return false;
    }
}