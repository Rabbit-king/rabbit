package first;

import java.util.Scanner;
import java.util.Stack;
//括号匹配
public class first {
	public static void main(String[] args) {
	Solution Slt=new Solution();
	Slt.input();
	if(Slt.isValid(Slt.s))
		System.out.println("True");
	else 
		System.out.println("False");
	}
}

class Solution {
	String s = "";

	public void input() {
		Scanner scan = new Scanner(System.in);

		s = scan.nextLine();
		scan.close();
	}

	public boolean isValid(String s) {
		Stack<Character> stc = new Stack<>();
		// 定义一个栈
		for (int i = 0; i < s.length(); i++) {
			if (s.charAt(i) == '[' || s.charAt(i) == '{' || s.charAt(i) == '(') {
				stc.push(s.charAt(i));
			}
			if (s.charAt(i) == ']') {
				if (s.charAt(i) == '[') {
					stc.pop();
					return false;
				}
			}
			if (s.charAt(i) == '}') {
				if (s.charAt(i) == '{') {
					stc.pop();
					return false;
				}
			}
			if (s.charAt(i) == ')') {
				if (s.charAt(i) == '(') {
					stc.pop();
					return false;
				}
			}
		}
		if (stc.empty())
			return true;
		else
			return false;
	}
}