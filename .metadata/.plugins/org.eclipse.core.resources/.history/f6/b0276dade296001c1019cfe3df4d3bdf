package second;
//给定一个字符串，验证它是否是回文串，只考虑字母和数字字符，可以忽略字母的大小写。
public class second {

}
class Solution {
    public boolean isPalindrome(String s) {
    	for(int i=0;i<128;i++) {
    		char j=(char)i;
    		if(!(('0'<=j&&j<='9')||('a'<=j&&j<='z')&&('A'<=j&&j<='Z'))) {
    			s=s.replaceAll(j, '');
    		}
    	}
    	
    	String s1=new StringBuffer(s).reverse().toString();
    	
    	
    	
		return false;
    }
}