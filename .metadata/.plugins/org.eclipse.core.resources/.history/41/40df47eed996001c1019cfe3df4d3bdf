class Solution {
    public String addBinary(String a, String b) {
    	if(a.equals("0")) {
    		return b;
    	}
    	else if(b.equals("0")) {
    		return a;
    	}
		
    	else {
    		StringBuffer s1=new StringBuffer(a);
    		StringBuffer s2=new StringBuffer(b);
    		
    	}
		
    	return null;
    }
}