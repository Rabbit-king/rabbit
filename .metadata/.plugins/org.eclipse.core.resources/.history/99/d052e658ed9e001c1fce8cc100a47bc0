package SequenceList;

public class LinkList<T> {
	private int N = 0;// Á´±í³¤¶È

	Node head = null;

	class Node{
		T value;
		Node next;
		public Node(T value, Node next) {
			this.value = value;
			this.next = next;
		}
	}

	public LinkList() {
		this.N = 0;
		this.head = new Node(null,null);
	}
	public void add(T t) {
		Node n=head;
		while(n.next!=null) {
			n=n.next;
		}
		n.next=t;
	}
	public void claer() {
		head.next=null;
	}
	public boolean isEmpty() {
		return N==0;
	}
	public int length() {
		return N;
	}
	public T get(int i) {
		return null;
		
	}
	
	

}
