package first;
//Ã°ÅÝÅÅÐò
public class maopao {
	public static void main(String[] args) {
		mp m=new mp();
		int[] a= {1,2,3};
		int[] a1=m.maopao(a1);
	}
}

class mp{
	public int[] maopao( int a[]) {
		int k=0;
		int temp=0;
		for(int i=a.length-1;i>0;i--) {
			for(int j=0;j<i;j++) {
				if(a[j]>a[j+1]) {
					temp=a[j];
					a[j]=a[j+1];
					a[j+1]=a[j];
					k++;
				}
			}
			if(k==0)
				break;
		}
		
		return a;
	}
}
