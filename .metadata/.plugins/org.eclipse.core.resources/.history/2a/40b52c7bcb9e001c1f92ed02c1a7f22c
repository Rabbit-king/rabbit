package SequenceList;

public class List<T>{
	private T[] a;
	
	private int N;
	
	List(int capital){
		this.a=(T[])new Object[capital];
		this.N=0;
	}
	
	public void clear() {	
		this.N=0;
	}
	
	public boolean isEmpty() {
		if(this.N==0)
			return true;
		else
			return false;	
	}
	
	public int length() {
		return this.N;	
	}
	
	public T get(int index) {
		try {
			if (index >= N)
				return null;
			else
				return this.a[index];
		} catch (Exception e) {
			// TODO: handle exception
			return null;
		}
	}
	
	public Boolean insert(int i,T append) {
		return false;
		
	}
	
	
}
