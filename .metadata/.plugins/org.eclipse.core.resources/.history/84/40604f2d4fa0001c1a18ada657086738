package joesefu;

import java.util.LinkedList;
import java.util.Scanner;

//约瑟夫问题，有n个人，从1开始报数，报到3的人退出，还剩下哪两个人

public class first {
	LinkedList<Integer> L=new LinkedList<Integer>();
	int n=0;
	
	public int left(int n) {
		for(int i=1;i<=n;i++) {
			L.add(i);
		}
		
		
		
		return 0;	
	}
	
	
	public static void main(String[] args) {
		first f=new first();
		f.left(5);
	}
	
}
